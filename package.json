{
  "name": "gatsby-starter-typescript-apollo-storybook",
  "author": "Tyler Churchill",
  "description": "A simplified bare-bones starter for Gatsby including typescript, apollo, storybook, and emotion.",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "storybook",
    "emotion",
    "styled",
    "apollo",
    "graphql"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "3.2.5",
    "@emotion/core": "10.1.1",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.25.2",
    "gatsby-plugin-emotion": "4.3.15",
    "isomorphic-fetch": "2.2.1",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.21",
    "@types/isomorphic-fetch": "0.0.35",
    "babel-loader": "8.1.0",
    "fork-ts-checker-webpack-plugin": "5.2.1",
    "gatsby-plugin-typescript": "2.5.0",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.7.2",
    "ts-loader": "8.0.8",
    "typescript": "3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tyler-Churchill/gatsby-starter-typescript-storybook"
  }
}
